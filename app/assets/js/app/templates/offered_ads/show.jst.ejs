<div id="offered-ad">
  <div class="main">
    <div class="title">
      <h3><%= model.get('title') %></h2>
    </div>
    <div class="details">
      <div class="company">
        公司: <%= model.get('company') %>
      </div>
      <div class="work-location">
        工作低点: <%= model.get('work_location').get('city') %>
      </div>
      <div class="date">
        发表于: <%= model.get('posted_at') %>
      </div>
    </div>
    <div class="body">
      <%= model.get('body') %>
    </div>
  </div>
</div>
