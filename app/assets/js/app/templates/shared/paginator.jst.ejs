<% if(info.pages > 1) { %>
  <a id="p-previous" class='<%= info.prev ? "" : "disabled" %>' href="#" accesskey="P">
    <%= TR('paginator.previous') %>
  </a>

    <select id="p-selection" accesskey="J">
    <% for(i=1;i<=info.pages;i++) { %>
      <option <%= info.page === i ? "selected" : "" %> value=<%= i %>><%= TR('paginator.page_no') %><%= i %></option>
    <% } %>
    </select> 

  <a id="p-next" class='<%= info.next ? "" : "disabled" %>' href="#" accesskey="N"><span class="accesskey"><%= TR('paginator.next') %></a>
<% } %>
