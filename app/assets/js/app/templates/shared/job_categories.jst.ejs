<form>
  <ul style="unstyled">
  <% for(var i=0;i < models.length;i++) { %>
    <li>
      <label><input type="checkbox" value="<%= models[i].get('id') %>" name="job-categories" />
      <%= TR('job_categories.' + models[i].get('code')) %></label>
    </li>
  <% } %>
  </ul>
  <button class='btn'><%= TR('actions.ok') %></button>
</form>
